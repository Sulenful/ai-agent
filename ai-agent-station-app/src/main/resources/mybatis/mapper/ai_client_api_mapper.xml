    <select id="selectList" parameterType="com.lingfeng.infrastructure.dao.po.AiClientApi" resultMap="BaseResultMap">
        SELECT <include refid="Base_Column_List"/>
        FROM ai_client_api
        <where>
            <if test="apiId != null and apiId != ''">
                AND api_id = #{apiId}
            </if>
            <if test="baseUrl != null and baseUrl != ''">
                AND base_url LIKE CONCAT('%', #{baseUrl}, '%')
            </if>
            <if test="status != null">
                AND status = #{status}
            </if>
        </where>
